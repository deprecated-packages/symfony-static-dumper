{
    "name": "symplify/symfony-static-dumper",
    "description": "Dump Symfony application to Static Website",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.3",
        "symfony/console": "^4.4|^5.1",
        "symfony/filesystem": "^4.4|^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symfony/http-foundation": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/config": "^4.4|^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/routing": "^4.4|^5.1",
        "symplify/autowire-array-parameter": "^9.0",
        "symplify/package-builder": "^9.0",
        "symplify/symplify-kernel": "^9.0",
        "symplify/smart-file-system": "^9.0",
        "symplify/console-package-builder": "^9.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "phpstan/phpstan": "^0.12.57",
        "phpunit/phpunit": "^8.5|^9.4",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/twig-bundle": "^4.4|^5.1",
        "symplify/autodiscovery": "^9.0",
        "symplify/flex-loader": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyStaticDumper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyStaticDumper\\Tests\\": "tests",
            "Symplify\\SymfonyStaticDumper\\Tests\\TestProject\\": "tests/test_project/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
