{
    "name": "symplify/symfony-static-dumper",
    "description": "Dump Symfony application to Static Website",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/routing": "^6.0",
        "symplify/autowire-array-parameter": "^11.1",
        "symplify/package-builder": "^11.1",
        "symplify/symplify-kernel": "^11.1",
        "symplify/smart-file-system": "^11.1"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13.3",
        "doctrine/cache": "^1.13",
        "phpstan/phpstan": "^1.8.1",
        "phpunit/phpunit": "^9.5.21",
        "symfony/framework-bundle": "^6.0.10",
        "symfony/twig-bundle": "^6.0.8"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyStaticDumper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyStaticDumper\\Tests\\": "tests",
            "Symplify\\SymfonyStaticDumper\\Tests\\TestProject\\": "tests/test_project/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.1-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<11.1.0",
        "symplify/composer-json-manipulator": "<11.1.0",
        "symplify/easy-coding-standard": "<11.1.0",
        "symplify/phpstan-rules": "<11.1.0",
        "symplify/easy-testing": "<11.1.0",
        "symplify/rule-doc-generator-contracts": "<11.1.0",
        "symplify/php-config-printer": "<11.1.0",
        "symplify/phpstan-extensions": "<11.1.0",
        "symplify/rule-doc-generator": "<11.1.0",
        "symplify/vendor-patches": "<11.1.0",
        "symplify/skipper": "<11.1.0",
        "symplify/monorepo-builder": "<11.1.0",
        "symplify/config-transformer": "<11.1.0",
        "symplify/easy-ci": "<11.1.0",
        "symplify/coding-standard": "<11.1.0",
        "symplify/easy-parallel": "<11.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
